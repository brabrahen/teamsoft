// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model User {
  id          String    @id @default(uuid())
  cnpj        String       @unique
  razaoSocial String
  nomeContato String
  telefone    Int
  address     Address[]
}

model Address {
  id          String @id @default(uuid())
  logradouro  String
  numero      Int
  complemento String
  bairro      String
  cidade      String
  estado      String
  cep         String
  user        User   @relation(fields: [userId], references: [id])
  userId      String
}
